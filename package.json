{
  "name": "kneden",
  "version": "0.3.0",
  "description": "Compile ES7 async/await to vanilla ES6 Promise chains",
  "main": "lib/index.js",
  "bin": "./bin/kneden",
  "directories": {
    "bin": "bin",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marten-de-vries/kneden.git"
  },
  "dependencies": {
    "acorn": "^3.0.2",
    "acorn-es7-plugin": "^1.0.11",
    "ast-hoist": "marten-de-vries/ast-hoist",
    "escodegen": "^1.8.0",
    "estraverse": "^4.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "colors": "^1.1.2",
    "diff": "^2.2.1",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4"
  },
  "scripts": {
    "test": "npm run lint && npm run unit && npm run coverage",
    "fast-test": "node test/integration/test.js",
    "coverage": "./node_modules/.bin/istanbul cover test/integration/test.js && ./node_modules/.bin/istanbul check-coverage --statements 100 --functions 100 --branches 100 --lines 100",
    "unit": "./node_modules/.bin/mocha --compilers js:babel-register test/unit",
    "lint": "./node_modules/.bin/eslint *.js test/integration/test.js && ./node_modules/.bin/eslint -c .eslintrc.tests.json test/integration/*.in.js test/integration/*.out.js test/unit/*.js"
  },
  "keywords": [
    "es7",
    "es6",
    "promise",
    "async",
    "await",
    "promises",
    "function",
    "functions"
  ],
  "author": "Marten de Vries",
  "license": "ISC"
}
